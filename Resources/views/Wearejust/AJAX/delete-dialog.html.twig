{% trans_default_domain "wearejust-templates" %}

<div class="sonata-ba-delete popup-file-delete mfp-hide">
    <div class="box box-danger">
        <div class="box-header">
            <h3 class="box-title">{{ 'dialog_confirm_deletion_title' | trans }}</h3>
        </div>
        <div class="box-body">
            {{ 'dialog_confirm_deletion_text' | trans }}
        </div>
        <div class="box-footer clearfix">
            <button class="btn btn-danger js-trigger-save"><i class="fa fa-trash" aria-hidden="true"></i>{{ 'dialog_confirm_deletion_yes' | trans }}</button>
            <button class="btn btn-success js-trigger-cancel" href="#">
                <i class="fa fa-pencil" aria-hidden="true"></i>
                {{ 'dialog_confirm_deletion_no' | trans }}
            </button>
        </div>
    </div>
